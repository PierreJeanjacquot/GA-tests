docker run --rm -v /var/run/docker.sock:/var/run/docker.sock registry.scontain.com/scone-production/iexec-sconify-image:5.9.0-v15 sconify_iexec --from=pierreiexec/hello-js:0.0.1 --to=pierreiexec/hello-js:scone --binary-fs --fs-dir=/app --host-path=/etc/hosts --host-path=/etc/resolv.conf --binary=/usr/local/bin/node --heap=1G --dlopen=1 --no-color --verbose --command="node /app/src/app.js"
